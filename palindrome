#include<iostream.h>
void main()
{
int n,i,j,k,count=0;
int a[];
cout<<"ENTER THE LIMIT OF ARRAY";
cin>>n;
for(i=0;i<n;++i)
{ cin>>a[i];
}
for(j=0,k=n;j<n/2;j++,k--)
{
if(a[j]==a[k])
count++;
}
if(count==n/2)
cout<<"THE GIVEN DIGIT IS A PALINDROME");
else
cout<<"THE GIVEN DIGIT IS NOT A PALINDROME");
}
